<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head.ejs');%>
    <title> <%= title%> </title>
</head>
<body>
    <%- include('../partials/nav.ejs');%>
      <div class="studentTable form">
        <select name="Block" id="select" onChange="selectedBlock()">
          <option value="none">None</option>
          <option value="A" >Block A</option>
          <option value="B">Block B</option>
          <option value="C">Block C</option>
          <option value="D">Block D</option>
          
        </select>
      <div id="TablePagingAreaA" style="display:none; padding: 0 20px 0 20px;"></div>
      <table id="TablePagingAreaB" style="display:none; padding: 0 20px 0 20px;"></table>
      <table id="TablePagingAreaC" style="display:none; padding: 0 20px 0 20px;"></table>
      <table id="TablePagingAreaD" style="display:none; padding: 0 20px 0 20px;"></table>
    </div>
    <!-- Button trigger modal -->
<!-- <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModalCenter">
  Launch demo modal
</button> -->

<!-- Modal -->
<div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Student Details</h5>
        <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body SName" id="infoModalBody"></div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" id="modalRemoveStudent">Remove Student</button>
      </div>
    </div>
  </div>
</div>
</body>
</html>

<script>
  var roomInfo = <%- roomInfo%>
</script>
